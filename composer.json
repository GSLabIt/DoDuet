{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gmp": "^8.1",
        "ext-openssl": "^8.1",
        "ext-sodium": "^8.1",
        "do-inc/laravel-persona-kyc": "^0.10.20",
        "do-inc/referral-module": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "hedii/laravel-gelf-logger": "^7.0",
        "illuminate/contracts": "^9.0",
        "inertiajs/inertia-laravel": "^0.5.4",
        "joshbrw/laravel-module-installer": "^2.0",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.4",
        "laravel/octane": "^1.0",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "nwidart/laravel-modules": "^9.0",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-permission": "^5.1",
        "spatie/regex": "^3.1.1",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.3",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Doinc\\Modules\\Commander\\": "modules/Commander/src/",
            "Doinc\\Modules\\Referral\\": "modules/Referral/src/",
            "Doinc\\Modules\\Referral\\Database\\Factories\\": "modules/Referral/database/factories/",
            "Doinc\\Modules\\Referral\\Database\\Seeders\\": "modules/Referral/database/seeders/"
        },
        "files": [
            "app/Helpers/index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:models -M -W -D app -D modules"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "module-dir": "modules"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "joshbrw/laravel-module-installer": true,
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Do-inc/laravel-module-referral.git"
        }
    ]
}
